bs_package_flags = -bs-package-map easy-drive
src_root_dir = D:\programming\drive
bsc = C:\Users\Ilmari\AppData\Roaming\npm\node_modules\bs-platform\lib\bsc.exe
bsdep = C:\Users\Ilmari\AppData\Roaming\npm\node_modules\bs-platform\lib\bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I "D:\programming\drive\node_modules\bs-platform\lib\ocaml" -no-alias-deps -color always
ppx_flags = 
bs_package_includes = -I D:\programming\drive\node_modules\reason-react\lib\ocaml
bs_package_dev_includes = 
namespace = -ns EasyDrive
bsb_dir_group = 0
refmt = C:\Users\Ilmari\AppData\Roaming\npm\node_modules\bs-platform\lib\refmt3.exe
reason_react_jsx = -ppx "C:\Users\Ilmari\AppData\Roaming\npm\node_modules\bs-platform\lib\reactjs_jsx_ppx_2.exe"
refmt_flags = --print binary
bsc_lib_includes = -I . -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src\index.mlast : build_ast_and_module_sets_from_re $src_root_dir\src\index.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src\index.mlast.d : build_deps src\index.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src\index-EasyDrive.cmj |  $src_root_dir\lib\js\src\index.bs.js src\index-EasyDrive.cmi : build_cmj_cmi src\index.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib\js\src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build EasyDrive.cmi : build_package EasyDrive.mlmap
build build.ninja : phony  ||  EasyDrive.cmi src\index.mlast.d
